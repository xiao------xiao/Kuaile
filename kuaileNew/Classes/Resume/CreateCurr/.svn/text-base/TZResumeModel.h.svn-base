#import <Foundation/Foundation.h>

@interface TZResumeModel : NSObject

/** 简历名称 */
@property (nonatomic, copy) NSString *resume_name;
/** 简历ID */
@property (nonatomic, copy) NSString *resume_id;
/** 头像avatar */
@property (nonatomic, copy) NSString *avatar;
/** 更新时间 */
@property (nonatomic, copy) NSString *updated;
/** 是否是默认简历 */
@property (nonatomic, copy) NSNumber *isDefault;

#pragma mark 简历基本信息
/** 真实姓名 */
@property (nonatomic, copy) NSString *name;
/** 专业 */
@property (nonatomic, copy) NSString *major;
/** 性别 */
@property (nonatomic, copy) NSString *gender;
/** 出生年份 */
@property (nonatomic, copy) NSString *birthday; // 出生日期 年
/** 出生月份 */
@property (nonatomic, copy) NSString *month;
/** 当前省份 */
@property (nonatomic, copy) NSString *link_provice;
/** 当前城市 */
@property (nonatomic, copy) NSString *link_city;
/** 开始工作 */
@property (nonatomic, copy) NSString *startWorkTime;
/** 手机号码 */
@property (nonatomic, copy) NSString *telephone;
/** 最高学历 */
//@property (nonatomic, copy) NSString *degree;
/** 工作年限 */
@property (nonatomic, copy) NSString *work_exp;
/** 邮箱 */
@property (nonatomic, copy) NSString *mail;
/** 个人介绍 */
@property (nonatomic, copy) NSString *profile;
/** 简历照片url */
@property (nonatomic, copy) NSString *imageUrl;
/** 简历照片本地name */
@property (nonatomic, copy) NSString *imageName;

#pragma mark 学历信息
/** 学校名称 */
@property (nonatomic, copy) NSString *school;
/** 入学时间 */
@property (nonatomic, copy) NSString *edu_start;
/** 毕业时间 */
@property (nonatomic, copy) NSString *edu_end;
/** 所学专业 */
// @property (nonatomic, copy) NSString *major;
/** 学历 */
@property (nonatomic, copy) NSString *degree;

#pragma mark 工作经验
@property (nonatomic, strong) NSArray *workExps;

#pragma mark 求职意向
/** 期望工作区域 市 */
@property (nonatomic, copy) NSString *hope_city;
/** 期望工作区域 县 */
@property (nonatomic, copy) NSString *hope_town;
/** 期望薪资 */
@property (nonatomic, copy) NSString *hope_salary;
/** 期望职位类别 */
@property (nonatomic, copy) NSString *hope_career;
/** 期望职位类别 二级 */
@property (nonatomic, copy) NSString *hope_career_t;

#pragma mark 自动投递
@property (nonatomic, copy) NSString *deliver;

@end
